{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "jsx": "react-native",
    "noEmit": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "exclude": [
    "node_modules",
  ]
}
